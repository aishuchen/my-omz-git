# pull
alias gpl='git pull'
alias gplrb='git pull --rebase'
alias gplrbv='git pull --rebase -v'
alias gplrba='git pull --rebase --autostash'
alias gplrbav='git pull --rebase --autostash -v'
alias gplrbupm='git pull --rebase origin $(git_main_branch)'
alias gplrbupmi='git pull --rebase=interactive origin $(git_main_branch)'
alias gplupm='git pull upstream $(git_main_branch)'
alias gplupc='git pull upstream $(git_current_branch)'
alias gploc='git pull origin "$(git_current_branch)"'
